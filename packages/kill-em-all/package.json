{
  "name": "kill-em-all",
  "version": "0.0.5",
  "description": "A utility to kill a process and all its child processes and wait for them to exit",
  "keywords": [
    "kill",
    "linux",
    "macos",
    "process",
    "process tree",
    "signal",
    "terminate",
    "tree",
    "windows",
    "wmic"
  ],
  "license": "MIT",
  "repository": "github:cyco130/kill-em-all",
  "bin": {
    "kill-em-all": "./cli.js"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "type": "module",
  "typesVersions": {
    "*": {
      "*": [
        "dist/*.d.ts"
      ]
    }
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "tsdown --clean",
    "dev": "tsdown --watch",
    "prepack": "rm -rf dist && pnpm build",
    "test": "pnpm run /^test:/",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "test:lint": "eslint . --max-warnings 0 --ignore-pattern dist",
    "test:package": "publint"
  },
  "devDependencies": {
    "@cyco130/eslint-config": "^6.2.1",
    "@types/node": "^20.19.35",
    "eslint": "^10.0.2",
    "publint": "^0.3.17",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": "20 || 22 || 24 || 25"
  }
}
